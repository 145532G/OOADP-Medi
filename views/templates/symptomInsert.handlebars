<link rel="stylesheet" href="/css/symptomchecker.css">
<div class="container">
    <div class="form">
        <form action="/symptom/symptomInsert" method="POST" class="formInformation">
            <label>Type what question you want to insert</label><br>
            <input type="text" name="symptomquestioninsert" placeholder="Type the question here"><br>
            <input type="submit" value="Submit">
        </form>
    </div>
</div>
{{#if result}}
{{#each result}}
<div class="container">
    <div class="form">
        <div class="inline">
            <form action="/symptom/symptomDestroy/{{this.id}}" method="POST">
                <input type="submit" value="Delete">   
            </form>
            <form action="/symptom/symptomEdit" method="POST">
                <input type="submit" value="Edit">
            </form>
        </div>  
        <p>{{question}}</p>
    </div>
</div>
{{/each}}
{{/if}}