<link rel="stylesheet" href="/css/symptomchecker.css">
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

<div class="wholeForm">
    <div class="symptomAnswer">
            <h1 id="symptomAnswer">{{result_symptom}}</h1>
    </div>
    <div class="symptomNotes">
            <p id="symptomNotes">{{result_text}}</p>
    </div>
    <div class="symptomRecommend">
            <p id="symptomRecommend">{{result_recommend}}</p>
    </div>
    <div class="symptomButtons">
        <form class="symptomFormButton">
            <input type="submit" value="No Thanks" id="submitNT" />
            <input type="submit" value="Book Appointment" id="submitBA" />
        </form>
    </div>
</div>